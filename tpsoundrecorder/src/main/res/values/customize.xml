<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- the prefix of recordings files name.prefix like this "Rec",default value is null -->
    <string name="def_save_name_prefix" translatable="false"></string>

    <!--
         the name format of recordings files,format like this "yyyy-MM-dd-HH-mm-ss",
            default value is null
    -->
    <string name="def_save_name_format" translatable="false"></string>
    <string name="def_date_format">"yyyy-MM-dd-HH-mm-ss"</string>

    <!--
         the type of saved recordings files.The value is 0-amr , 1-3gpp , 2-wav.Default is 0-amr.
            default value is 0
    -->
    <integer name="def_save_type" translatable="false">0</integer>

    <!--
         the mimetype of saved recordings files.The value is "audio/amr","audio/3gpp",
            "audio/wave_2ch_lpcm".Default is "audio/amr".
    -->
    <string name="def_save_mimetype" translatable="false">audio/aac_mp4</string>
    <bool name= "config_storage_path">false</bool>
</resources>
